module programa1 {

	mx; // attribute mx
	mn; // attribute mn
	data=[100]; // vector of 100 integers

function det(d[]) {
	i=0;
	M=d.size-1;
	while(i<M) {
		a=d[i];
		i=i+1;
		b=d[i];
		mx= library1.max(a,b);
		mn= library1.min(a,b);
	}
}

function main() {
	det(data);

	io.println("max: ",mx); 
	io.println("min: ",mn);
}
}

